{
  "multipart": [
    {
      "when": {"active": "false"},
      "apply": {"model": "rebornmod:high_security_fence_pole_inactive.tbl"}
    },
    {
      "when": {"active": "true"},
      "apply": {"model": "rebornmod:high_security_fence_pole_active.tbl"}
    },
    {
      "when": {"north": "true"},
      "apply": {"model": "rebornmod:high_security_fence_pole_wires.tbl", "y": 270}
    },
    {
      "when": {"west": "true"},
      "apply": {"model": "rebornmod:high_security_fence_pole_wires.tbl", "y": 180}
    },
    {
      "when": {"south": "true"},
      "apply": {"model": "rebornmod:high_security_fence_pole_wires.tbl", "y": 90}
    },
    {
      "when": {"east": "true"},
      "apply": {"model": "rebornmod:high_security_fence_pole_wires.tbl"}
    }
  ]
}